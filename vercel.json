{
    "version": 2,
    "builds": [
      {
        "src": "src/api.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/api.ts"
      }
    ],
    "crons": [
       {
         "path": "/categories/run-cron",
         "schedule": "* * * * *" 
       }
     ]
  }
  